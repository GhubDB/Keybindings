{
  "title": "PROgramming",
  "rules": [
    {
      "description": "ö to {",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.microsoft\\.VSCode$",
                "^com\\.jetbrains\\.rider$",
                "^com\\.microsoft\\.visual-studio$"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "semicolon"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "8",
              "modifiers": ["left_alt"]
            }
          ]
        }
      ]
    },
    {
      "description": "shift ö to }",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.microsoft\\.VSCode$",
                "^com\\.jetbrains\\.rider$",
                "^com\\.microsoft\\.visual-studio$"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "modifiers": {
              "mandatory": ["left_shift"]
            },
            "key_code": "semicolon"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "9",
              "modifiers": ["left_alt"]
            }
          ]
        }
      ]
    },
    {
      "description": "ä to `",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.microsoft\\.VSCode$",
                "^com\\.jetbrains\\.rider$",
                "^com\\.microsoft\\.visual-studio$"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "quote"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "equal_sign",
              "modifiers": ["left_shift"]
            },
            {
              "key_code": "spacebar"
            }
          ]
        }
      ]
    },
    {
      "description": "shift + ä to ^",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.microsoft\\.VSCode$",
                "^com\\.jetbrains\\.rider$",
                "^com\\.microsoft\\.visual-studio$"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "modifiers": {
              "mandatory": ["left_shift"]
            },
            "key_code": "quote"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "equal_sign"
            }
          ]
        }
      ]
    },
    {
      "description": "ü to [",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.microsoft\\.VSCode$",
                "^com\\.jetbrains\\.rider$",
                "^com\\.microsoft\\.visual-studio$"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "open_bracket"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "5",
              "modifiers": ["left_alt"]
            }
          ]
        }
      ]
    },
    {
      "description": "shift + ü to ]",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.microsoft\\.VSCode$",
                "^com\\.jetbrains\\.rider$",
                "^com\\.microsoft\\.visual-studio$"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "modifiers": {
              "mandatory": ["left_shift"]
            },
            "key_code": "open_bracket"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "6",
              "modifiers": ["left_alt"]
            }
          ]
        }
      ]
    }
  ]
}
