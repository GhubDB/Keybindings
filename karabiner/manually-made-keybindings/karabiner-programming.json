{
  "title": "PROgramming",
  "rules": [
    {
      "description": "Hyper Key (⌃⌥⇧⌘)",
      "manipulators": [
        {
          "description": "Caps Lock -> Hyper Key",
          "from": {
            "key_code": "caps_lock"
          },
          "to": [
            {
              "key_code": "left_shift",
              "modifiers": ["left_command", "left_control", "left_option"]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Open -a 'Google Chrome.app'",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome.app'"
            }
          ]
        }
      ]
    },
