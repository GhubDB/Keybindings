    {
      "description": "ö to {",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.microsoft\\.VSCode$",
                "^com\\.jetbrains\\.rider$",
                "^com\\.microsoft\\.visual-studio$",
                "^com\\.azuredatastudio.oss$"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "semicolon"
          },
          "to": [
            {
              "repeat": true,
              "key_code": "8",
              "modifiers": ["left_alt"]
            }
          ]
        }
